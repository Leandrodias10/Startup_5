{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
"files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".vscode": true,
    "constants": true,
    "hooks": true,
    "scripts": true,
    ".gitignore":true,
    "app.json": true,
    "package-lock.json": true,
    "package.json": true,
    "components": true,
    "eslint.config.js": true,
    "README.md": true,
    "tsconfig.json": true,
    ".expo": true,
    "node_modules": true,
    "expo-env.d.ts": true
  }
}
